{
  "author": "support@purpledotprice.com",
  "name": "@purple-dot/browser",
  "description": "Purple Dot Browser SDK",
  "license": "Apache-2.0",
  "version": "0.0.2",
  "scripts": {
    "build": "tsc",
    "lint": "rome ci src/",
    "format": "rome check --apply-unsafe src/ && rome format --write src/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cy:open": "cd cypress/support && yarn dlx http-server . -p 8080& cd ../.. && cypress open",
    "cy:run": "cd cypress/support && yarn webpack --config webpack.config.js && yarn dlx http-server . -p 8080 & cd ../.. && cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/purple-dot/browser.git"
  },
  "bugs": {
    "url": "https://github.com/purple-dot/browser/issues"
  },
  "homepage": "https://github.com/purple-dot/browser#readme",
  "main": "dist/index.js",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/js-cookie": "^3.0.3",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-v8": "^0.33.0",
    "cypress": "^12.17.1",
    "html-webpack-plugin": "^5.5.3",
    "jsdom": "22.1.0",
    "rome": "12.1.3-nightly.f65b0d9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "whatwg-fetch": "^3.6.2"
  },
  "engines": {
    "node": "18.16.x",
    "yarn": "3.6.x"
  },
  "packageManager": "yarn@3.6.1",
  "dependencies": {
    "js-cookie": "^3.0.5",
    "uuid": "^9.0.0"
  }
}
